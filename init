local Loader = shared.KrampusLoader
if not Loader then
	Loader = Instance.new("ModuleScript")
	Loader.Name = "Loader"
	Loader.Source = game:HttpGet("https://raw.githubusercontent.com/iamtryingtofindname/Krampus/main/loader",true)
	shared.KrampusLoader = Loader
end
Loader = require(Loader)
Loader(-1,"Template",function(initTopBarButton,topLeft,topRight,extra)
	local ToggleButton = initTopBarButton(function()
		print("pressed")
	end) -- Makes a topbar button
	ToggleButton.Parent = topLeft
end,function()
	print("Code ran")
end)
